<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow unsigned executable memory for LLM inference -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Allow JIT compilation for inference engines -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Allow dynamic loading of libraries -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
    
    <!-- Disable library validation for external dependencies -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Network access for downloading models and server communication -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <key>com.apple.security.network.server</key>
    <true/>
    
    <!-- File system access for model storage and configuration -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
    
    <key>com.apple.security.files.downloads.read-write</key>
    <true/>
    
    <!-- Hardware access for Metal GPU acceleration -->
    <key>com.apple.security.device.audio-input</key>
    <false/>
    
    <key>com.apple.security.device.camera</key>
    <false/>
    
    <!-- Allow hardened runtime but with necessary exceptions -->
    <key>com.apple.security.cs.debugger</key>
    <false/>
</dict>
</plist>
